{
  "name": "converter-react",
  "version": "0.0.1",
  "description": "Converter application (React)",
  "scripts": {
    "lint-client": "eslint --ext .js,.jsx --rule 'no-extra-parens: 0' client",
    "lint-server": "eslint --env node --rule 'no-var: 0' server *.js",
    "lint": "npm run lint-server && npm run lint-client",
    "test": "npm run lint",
    "start": "node server/index.js",
    "server": "nodemon --watch client --watch server --ext js,jsx server/index.js",
    "server-dev": "webpack-dev-server --config webpack.config.dev.js --progress --colors --port 2992",
    "server-hot": "webpack-dev-server --config webpack.config.hot.js --hot --progress --colors --port 2992 --inline",
    "sources": "http-server -p 3001 .",
    "watch": "webpack --watch",
    "prod": "npm run watch & npm run server & npm run sources",
    "dev": "npm run server-dev & WEBPACK_DEV=true npm run server",
    "hot": "npm run server-hot & WEBPACK_DEV=true npm run server",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/converter-react.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "example"
  ],
  "author": "Ryan Roemer <ryan.roemer@formidablelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/converter-react/issues"
  },
  "homepage": "https://github.com/FormidableLabs/converter-react",
  "dependencies": {
    "alt": "^0.16.2",
    "babel": "^5.1.13",
    "babel-core": "^5.1.13",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.1.13",
    "clean-webpack-plugin": "^0.1.2",
    "compression": "^1.4.3",
    "es6-promise": "^2.1.1",
    "express": "^4.12.3",
    "express-handlebars": "^2.0.1",
    "isomorphic-fetch": "^2.0.2",
    "react": "^0.13.2",
    "react-bootstrap": "^0.21.1",
    "webpack": "^1.8.11",
    "webpack-stats-plugin": "0.0.4"
  },
  "devDependencies": {
    "eslint": "^0.20.0",
    "eslint-plugin-react": "^2.2.0",
    "http-server": "^0.8.0",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^1.2.5",
    "webpack-dev-server": "^1.8.2"
  }
}
