{
  "name": "converter-react",
  "version": "0.0.1",
  "description": "Converter application (React)",
  "scripts": {
    "lint-client": "eslint --ext .js,.jsx -c .eslintrc-client client",
    "lint-client-test": "eslint --ext .js,.jsx -c .eslintrc-client-test test/client",
    "lint-server": "eslint -c .eslintrc-server server",
    "lint-server-test": "eslint -c .eslintrc-server-test test/server",
    "lint": "npm run lint-server && npm run lint-client && echo 'TODO lint server(20)/client(19) tests'",
    "test": "npm run lint",
    "start": "node server/index.js",
    "server": "nodemon --watch client --watch server --ext js,jsx server/index.js",
    "server-dev": "webpack-dev-server --config webpack.config.dev.js --progress --colors --port 2992",
    "server-hot": "webpack-dev-server --config webpack.config.hot.js --hot --progress --colors --port 2992 --inline",
    "sources": "http-server -p 3001 .",
    "watch": "webpack --watch",
    "prod": "npm run watch & npm run server & npm run sources",
    "dev": "npm run server-dev & WEBPACK_DEV=true npm run server",
    "hot": "npm run server-hot & WEBPACK_DEV=true npm run server",
    "build": "webpack",
    "postinstall": "node heroku/scripts/not-heroku.js || (node heroku/scripts/install.js && npm run build)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/converter-react.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "example"
  ],
  "author": "Ryan Roemer <ryan.roemer@formidablelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/converter-react/issues"
  },
  "homepage": "https://github.com/FormidableLabs/converter-react",
  "dependencies": {
    "alt": "^0.17.1",
    "babel": "^5.1.13",
    "babel-core": "^5.1.13",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.1.13",
    "bluebird": "^2.9.34",
    "clean-webpack-plugin": "^0.1.2",
    "compression": "^1.4.3",
    "express": "^4.12.3",
    "express-handlebars": "^2.0.1",
    "isomorphic-fetch": "^2.0.2",
    "react": "^0.13.2",
    "react-bootstrap": "^0.24.2",
    "webpack": "^1.8.11",
    "webpack-stats-plugin": "0.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "eslint": "^0.24.1",
    "eslint-config-defaults": "^3.1.0",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.1.0",
    "http-server": "^0.8.0",
    "jade": "^1.9.2",
    "marked": "^0.3.3",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^1.2.5",
    "recluster": "^0.4.0",
    "webpack-dev-server": "^1.8.2"
  }
}
